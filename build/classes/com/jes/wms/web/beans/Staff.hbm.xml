<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jes.wms.web.beans">
	<class name="Staff" table="SYS_STAFF">
		<id name="id" column="IKey" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="code" type="string" column="Staff_Code"
			not-null="true" length="32" />			
		<property name="name" type="string" column="Staff_Name"
			not-null="false" length="32" />
		<property name="pwd" type="string" column="Pwd"
			not-null="false" length="32" />
		<property name="mail" type="string" column="Mail"
			not-null="false" length="32" />
		<property name="mobilePhone" type="string" column="Mobile_Phone"
			not-null="false" length="32" />
		<property name="deptId" type="string" column="SYS_GROUP_Ikey"
			not-null="false" length="32" />
		<property name="status" type="boolean" column="Status"
			not-null="false" length="1" />
		<property name="org" type="string" column="Org" not-null="false"
			length="32" />
		<property name="createBy" type="string" column="Create_By"
			not-null="false" length="32" />
		<property name="createDate" type="date" column="Create_Date"
			not-null="false" />
		<property name="updateBy" type="string" column="Update_By"
			not-null="false" length="32" />
		<property name="updateDate" type="date" column="Update_Date"
			not-null="false" />
		<property name="isOutline" type="boolean" column="Out_line"
			not-null="false" length="1" />
	</class>
</hibernate-mapping>

